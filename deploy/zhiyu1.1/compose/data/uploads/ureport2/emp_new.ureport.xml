   "SELECT u.user_id, u.dept_id, d.dept_name ,u.nick_name, p.post_name from  sys_user u  
	join   sys_user_post  up on u.user_id = up.user_id
	join   sys_post p on up.post_id = p.post_id
    join   sys_dept d on  u.dept_id = d.dept_id 
    where u.del_flag =0 "  +   (emptyparam("deptId") == false ? " and  u.dept_id = :deptId " :"" )
 }]]></sql><field name="user_id"/><field name="dept_id"/><field name="dept_name"/><field name="nick_name"/><field name="post_name"/><parameter name="deptId" type="String" default-value=""/></dataset><dataset name="hour" type="sql"><sql><![CDATA[ select  d.hour_id,d.user_id, d.fill_date, s.work_type_id, t.name, d.use_hour, p.project_name 
  from mh_hour_detail  d 
  join mh_hour_detail_sub s  on d.id = s.detail_id
  join mh_work_type  t  on s.work_type_id = t.id
  join sys_project p on d.project_id = p.project_id
  WHERE (d.review_status = 0 ||  d.review_status = 2) and
  d.fill_date BETWEEN :startDate  and :endDate]]></sql><field name="hour_id"/><field name="user_id"/><field name="fill_date"/><field name="work_type_id"/><field name="name"/><field name="use_hour"/><field name="project_name"/><parameter name="startDate" type="String" default-value=""/><parameter name="endDate" type="String" default-value=""/></dataset></datasource><paper type="A4" left-margin="90" right-margin="90"
