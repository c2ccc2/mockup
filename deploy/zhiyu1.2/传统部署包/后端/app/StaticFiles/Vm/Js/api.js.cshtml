import request from '@@/utils/request'

// 查询@(Model.FunctionName)列表
export function list@(Model.BusinessName)(data) {
  return request({
    url: '/@(Model.moduleName)/@(Model.businessName)/list',
    method: 'post',
    data: data
  })
}

// 查询@(Model.FunctionName)详细
export function get@(Model.BusinessName)(@(Model.PkColumn_netField)) {
  return request({
    url: '/@(Model.moduleName)/@(Model.businessName)/Get/' + @(Model.PkColumn_netField),
    method: 'get'
  })
}

// 新增@(Model.FunctionName)
export function add@(Model.BusinessName)(data) {
  return request({
    url: '/@(Model.moduleName)/@(Model.businessName)/Add',
    method: 'post',
    data: data
  })
}

// 修改@(Model.FunctionName)
export function update@(Model.BusinessName)(data) {
  return request({
    url: '/@(Model.moduleName)/@(Model.businessName)/Edit',
    method: 'put',
    data: data
  })
}

// 删除@(Model.FunctionName)
export function del@(Model.BusinessName)(@(Model.PkColumn_netField)) {
  return request({
    url: '/@(Model.moduleName)/@(Model.businessName)/Remove/' + @(Model.PkColumn_netField)+'',
    method: 'delete'
  })
}
